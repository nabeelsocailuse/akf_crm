 "client_script": "frappe.web_form.after_load = () => {\n    frappe.web_form.set_value('institute', 'Riphah International University');\n}\n\nfrappe.web_form.on('gender', (field, value) => {\n    const gender = frappe.web_form.get_value(\"gender\");\n    const institute = frappe.web_form.get_value(\"institute\");\n    const intended_level_of_study = frappe.web_form.get_value(\"intended_level_of_study\");\n    load_filtered_programs(gender, institute, intended_level_of_study);\n});\n\nfrappe.web_form.on('institute', (field, value) => {\n    const gender = frappe.web_form.get_value(\"gender\");\n    const institute = frappe.web_form.get_value(\"institute\");\n    const intended_level_of_study = frappe.web_form.get_value(\"intended_level_of_study\");\n    load_filtered_programs(gender, institute, intended_level_of_study);\n});\n\nfrappe.web_form.on('intended_level_of_study', (field, value) => {\n    const gender = frappe.web_form.get_value(\"gender\");\n    const institute = frappe.web_form.get_value(\"institute\");\n    const intended_level_of_study = frappe.web_form.get_value(\"intended_level_of_study\");\n    load_filtered_programs(gender, institute, intended_level_of_study);\n});\n\nfrappe.web_form.on('select_field_study_program', (field, value) => {\n    frappe.web_form.set_value('proposed_program_of_study', value);\n});\n\n// NEW: When Preferred City changes, load programs for that city\nfrappe.web_form.on('preferred_city', (field, value) => {\n    const city = frappe.web_form.get_value(\"preferred_city\");\n    if (city) {\n        load_programs_in_city(city);\n    }\n});\n\nfunction load_filtered_programs(gender, institute, intended_level_of_study) {\n    frappe.call({\n        \"method\": \"akf_crm.akf_crm.apis.webform_api.get_filtered_programs\",\n        \"async\": false,\n        \"args\": {\n            \"gender\": gender,\n            \"institute\": institute,\n            \"intended_level_of_study\": intended_level_of_study\n        },\n        \"callback\": function (r) {\n            const options = r.message;\n            load_options(options);\n        }\n    });\n}\n\nfunction load_options(options) {\n    $('select[data-fieldname=\"select_field_study_program\"]').empty();\n    $('select[data-fieldname=\"select_field_study_program\"]').append(new Option(\"\", \"\"));\n    options.forEach(opt => {\n        $('select[data-fieldname=\"select_field_study_program\"]').append(new Option(opt.text, opt.value));\n    });\n}\n\nfunction load_programs_in_city(city) {\n    frappe.call({\n        \"method\": \"akf_crm.akf_crm.apis.webform_api.get_programs_in_city\",\n        \"async\": false,\n        \"args\": {\n            \"city\": city\n        },\n        \"callback\": function (r) {\n            const options = r.message;\n            load_city_program_options(options);\n        }\n    });\n}\n\nfunction load_city_program_options(options) {\n    $('select[data-fieldname=\"programs_in_city\"]').empty();\n    $('select[data-fieldname=\"programs_in_city\"]').append(new Option(\"\", \"\"));\n    options.forEach(opt => {\n        $('select[data-fieldname=\"programs_in_city\"]').append(new Option(opt.text, opt.value));\n    });\n}\n",
